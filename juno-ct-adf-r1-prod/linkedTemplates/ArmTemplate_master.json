{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "juno-ct-adf-r1-prod"
		},
		"Content_Forecasting_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Content_Forecasting'"
		},
		"Content_Handoff_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Content_Handoff'"
		},
		"Content_LocConfig_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Content_LocConfig'"
		},
		"Content_OpsConnector_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Content_OpsConnector'"
		},
		"Content_OpsConnector_LCS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Content_OpsConnector_LCS'"
		},
		"junoctfistr1prod_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'junoctfistr1prod'"
		},
		"junoctopscstr1prod_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'junoctopscstr1prod'"
		},
		"adeFileTracking_properties_typeProperties_endpoint": {
			"type": "string",
			"defaultValue": "https://junoctader1prod.eastus.kusto.windows.net"
		},
		"adeFileTracking_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "FinancialForecasting"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "juno-ct-adf-r1-prod_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Content_Forecasting_connectionString": {
						"value": "[parameters('Content_Forecasting_connectionString')]"
					},
					"Content_Handoff_connectionString": {
						"value": "[parameters('Content_Handoff_connectionString')]"
					},
					"Content_LocConfig_connectionString": {
						"value": "[parameters('Content_LocConfig_connectionString')]"
					},
					"Content_OpsConnector_connectionString": {
						"value": "[parameters('Content_OpsConnector_connectionString')]"
					},
					"Content_OpsConnector_LCS_connectionString": {
						"value": "[parameters('Content_OpsConnector_LCS_connectionString')]"
					},
					"junoctfistr1prod_connectionString": {
						"value": "[parameters('junoctfistr1prod_connectionString')]"
					},
					"junoctopscstr1prod_connectionString": {
						"value": "[parameters('junoctopscstr1prod_connectionString')]"
					},
					"adeFileTracking_properties_typeProperties_endpoint": {
						"value": "[parameters('adeFileTracking_properties_typeProperties_endpoint')]"
					},
					"adeFileTracking_properties_typeProperties_database": {
						"value": "[parameters('adeFileTracking_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "juno-ct-adf-r1-prod_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'juno-ct-adf-r1-prod_ArmTemplate_0')]"
			]
		},
		{
			"name": "juno-ct-adf-r1-prod_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'juno-ct-adf-r1-prod_ArmTemplate_1')]"
			]
		}
	]
}